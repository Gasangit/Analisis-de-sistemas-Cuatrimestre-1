package azar;

import javax.swing.JOptionPane;

class Azar {

	public static void main(String[] args) {
		double a,b,c;
		int c1;
		
		a= Math.random();
		b= Math.random()*10;		
		c= Math.random()*5000;
		
		c1=(int)c;
		
		JOptionPane.showMessageDialog(null, "Entre 0 y 0.9999: " +a+"\nEntre 0 y 9.9999: "+b+"\nEntre 0 y 4999.9: "+c);
	}

}
