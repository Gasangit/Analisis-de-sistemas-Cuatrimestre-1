<?xml version="1.0" encoding="UTF-8"?>
<root xmlns:nsd="https://structorizer.fisch.lu" version="3.32-04" preRepeat="until " postFor="to" preReturn="return" postForIn="in" preWhile="while " output="OUTPUT" input="INPUT" preFor="for" preExit="exit" preLeave="leave" ignoreCase="true" preThrow="throw" preForIn="foreach" stepFor="by" namespace="" author="GASAN" created="2021-11-13" changedby="GASAN" changed="2021-11-13" origin="Structorizer 3.32-04" text="&#34;numero-revuelto&#34;" comment="&#34;&#34;" color="ffffff" type="program" style="nice">
	<children>
		<repeat text="&#34;do while jugar.EqualsIgnoreCase(&#34;&#34;si&#34;&#34;)&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
			<qRepeat>
				<instruction text="&#34;ingresar juego//&#34;&#34;Por favor indique &#34;,&#34;el juego que desea jugar (1 o 2): &#34;,&#34;1 - Adivina el número&#34;,&#34;2 - Revuelto de letras&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
				<case text="&#34;juego&#34;,&#34;1&#34;,&#34;2&#34;,&#34;default&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
					<qCase>
						<instruction text="&#34;mostrar &#34;&#34;Tiene 5 intentos para &#34;,&#34;adivinar un número del 0 al 100&#34;,&#34;que el sistema generará al azar. &#34;,&#34;En cada intento se le informara &#34;,&#34;si el número que menciona es menor &#34;,&#34;o mayor al que generó la maquina&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
						<instruction text="&#34;num =(int)(Math.Random()*101)&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
						<while text="&#34;while (intento!=5 &#34;,&#34;|| n_jugador==num)&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
							<qWhile>
								<instruction text="&#34;intento+=1&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
								<instruction text="&#34;ingresar n_jugador//&#34;,&#34;&#34;&#34;Intento: &#34;&#34; + intento&#34;,&#34;&#34;&#34;Indique su número&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
								<alternative text="&#34;if n_jugador&#60;0&#34;,&#34;||n_jugador&#62;100&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
									<qTrue>
										<instruction text="&#34;mostrar &#34;&#34;Debe ingresar&#34;,&#34;un número entre 0 y 100.&#34;,&#34;No se contará este intento&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
										<instruction text="&#34;intento-=1&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
									</qTrue>
									<qFalse>
										<alternative text="&#34;if n_jugador==num&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
											<qTrue>
												<instruction text="&#34;mostrar &#34;&#34;ACERTÓ &#34;,&#34;¡¡¡FELICITACIONES!!!&#34;,&#34;El número es &#34;&#34; + num&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
											</qTrue>
											<qFalse>
												<alternative text="&#34;if n_jugador&#60;num&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
													<qTrue>
														<instruction text="&#34;mostrar &#34;&#34;El número &#34;&#34; + &#34;,&#34;n_jugador + &#34;,&#34;&#34;&#34; es MENOR al generado&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
													</qTrue>
													<qFalse>
														<instruction text="&#34;mostrar &#34;&#34;El número &#34;&#34; + &#34;,&#34;n_jugador + &#34;,&#34;&#34;&#34; es MAYOR al generado&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
													</qFalse>
												</alternative>
											</qFalse>
										</alternative>
									</qFalse>
								</alternative>
							</qWhile>
						</while>
					</qCase>
					<qCase>
						<instruction text="&#34;lista&#34;,&#34;[&#34;&#34;alacran&#34;&#34;,&#34;&#34;jugador&#34;&#34;,&#34;,&#34;&#34;&#34;mezclar&#34;&#34;,&#34;&#34;pantalon&#34;&#34;,&#34;,&#34;&#34;&#34;estatico&#34;&#34;,&#34;&#34;remera&#34;&#34;,&#34;,&#34;&#34;&#34;vacaciones&#34;&#34;,&#34;&#34;montaña&#34;&#34;,&#34;,&#34;&#34;&#34;cataratas&#34;&#34;,&#34;&#34;paraiso&#34;&#34;]&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
						<instruction text="&#34;n_palabra = Math.Random()*10&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
						<instruction text="&#34;ingresar jug_palabra//&#34;,&#34;&#34;&#34;Adivine la palabra que&#34;,&#34;se forma con las letras mezcladas &#34;,&#34;en la imagen (no ingrese tildes)&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
						<alternative text="&#34;if jug_palabra.EqualsIgnoreCase&#34;,&#34;(lista[n_palabra])&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
							<qTrue>
								<instruction text="&#34;mostrar &#34;&#34;ACERTÓ &#34;,&#34;¡¡¡FELICITACIONES!!!&#34;&#34;&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
							</qTrue>
							<qFalse>
								<instruction text="&#34;mostrar &#34;&#34;NO ACERTÓ, &#34;,&#34;la palabra correcta es &#34;&#34; &#34;,&#34;+ lista[n_palabra]&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
							</qFalse>
						</alternative>
					</qCase>
					<qCase>
						<instruction text="&#34;mostrar &#34;&#34;Solo hay 2 &#34;,&#34;juegos disponibles&#34;,&#34;debe ingresar 1 o 2&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
					</qCase>
				</case>
			</qRepeat>
		</repeat>
	</children>
</root>