				<instruction text="&#34;n = Math.Random()*101&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
				<alternative text="&#34;if n &#62;=0 &amp;&amp; n&#60;=25&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
