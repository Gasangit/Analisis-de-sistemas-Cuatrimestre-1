package prueba_funciones;

import javax.swing.JOptionPane;

class Funcion2 {

	public static void main(String[] args) {
		int a, b, resultado;
		Funcion1 c1 = new Funcion1(); // Se genera una instancia de la clase Funcion1 para poder usar el método suma()
		
		a = Integer.parseInt(JOptionPane.showInputDialog("N1"));
		b = Integer.parseInt(JOptionPane.showInputDialog("N2"));
		// se generan los dos enteros
		
		 resultado = Integer.parseInt(c1.sumar(a,b));// se llama al metodo suma pasandole los dos enteros a sumar
		
		

	}

}
